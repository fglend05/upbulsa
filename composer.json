{
    "name": "glend/uplbbulsa",
    "description": "A raw PHP MVC project.",
    "type": "project",
    "require": {
        "php": "^7.4 || ^8.0",
        "google/apiclient": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Controllers\\": "app/controllers/",
            "Core\\": "app/core/",
            "Models\\": "app/models/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": []
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist"
    }
}
